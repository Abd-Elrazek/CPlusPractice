1)
template<class Object>
bool HashTable<Object>::remove(const Object& x) {
	auto & list = array[my_hash(x)];
	auto itr = find( begin(list), end(list), x );

	if(itr == end(list)) return false;

	list.erase(itr);
	--num;
	return true;
}

